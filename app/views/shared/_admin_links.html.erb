<div class="admin-links">
  <% if admin_signed_in? %>
    <div>
      You are signed in as <%= current_admin.email %>
    </div>
    <div>
      <%= link_to "Logout", destroy_admin_session_path, method: :delete %>
    </div>
  <% else %>
    <%= link_to "Register", new_admin_registration_path %>
    <%= link_to "Login", new_admin_session_path %>
  <% end %>
</div>